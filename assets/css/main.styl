@import reset

body
  font-family "Maven Pro", Arial, Helvetica, sans-serif;
  font-size 14pt
  color #555

nav
  float right
  padding 5px

.queue
  float left
  padding 20px
  margin-right 20px
  width 200px

#main
  padding 40px 20px
  margin-left 260px

section
  width 322px
  position relative

#logo
  float left
  margin 5px 0 0 -70px

h1
  font-family "Maven Pro", Arial, Helvetica, sans-serif;
  font-size 34px
  font-weight normal

.arrow-left
  position absolute
  top 10px
  left -9px
  width 0
  height 0
  border-top 10px solid transparent
  border-bottom 10px solid transparent
  border-right 10px solid #ccc

.input
  input
    color #666
    border none
    border 1px solid #ccc
    border-radius 10px
    font-size 32pt
    width 300px
    padding 5px 10px
    outline none

.list
  margin-top 15px
  ul
    list-style none
    li
      background-color #eee
      padding 5px
